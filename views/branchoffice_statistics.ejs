<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous">
<table class="table table-hover" border="1">
    <tr>
        <th>Branch ID</th>
        <th>Street Address</th>
        <th>City</th>
        <th>Province</th>                   
    </tr>
    <% table_info.branchoffice.branchid.forEach(function(val, i){ %>    
        <tr>
            <td><%= table_info['branchoffice'].branchid[i] %></td>
            <td><%= table_info['branchoffice'].streetaddress[i] %></td>
            <td><%= table_info['branchoffice'].city[i] %></td>
            <td><%= table_info['branchoffice'].province[i] %></td>
        </tr>
    <% }) %>
</table>
