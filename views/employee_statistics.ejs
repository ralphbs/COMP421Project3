<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous">
<table class="table table-hover" border="1">
    <tr>
        <th>Name</th>
        <th>Employee ID</th>
        <th>Salary</th>
        <th>Branch ID</th>
        <th>Street Address</th>
        <th>City</th>
        <th>Province</th>
        <th>Employed Since</th>                     
    </tr>
    <% table_info.employees.employeeid.forEach(function(val, i){ %>     
        <tr>
                <td><%= table_info['employees'].name[i] %></td>
                <td><%= table_info['employees'].employeeid[i] %></td>
                <td><%= table_info['employees'].salary[i] %></td>
                <td><%= table_info['employees'].branchid[i] %></td>
                <td><%= table_info['employees'].streetaddress[i] %></td>
                <td><%= table_info['employees'].city[i] %></td>
                <td><%= table_info['employees'].province[i] %></td>
                <td><%= table_info['employees'].employedsince[i] %></td>
        </tr>
    <% }) %>
</table>
